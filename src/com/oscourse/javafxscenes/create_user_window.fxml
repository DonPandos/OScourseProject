<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="200.0" prefWidth="300.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.oscourse.controllers.CreateUserWindowController">
   <children>
      <TextField fx:id="usernameField" layoutX="25.0" layoutY="24.0" prefWidth="250.0" promptText="Username" />
      <TextField fx:id="passwordField" layoutX="25.0" layoutY="62.0" prefWidth="250.0" promptText="Password" />
      <CheckBox fx:id="adminCheck" alignment="CENTER_RIGHT" layoutX="25.0" layoutY="100.0" mnemonicParsing="false" prefHeight="19.0" prefWidth="75.0" text="Admin" textFill="#e9e9e9">
         <font>
            <Font size="14.0" />
         </font>
      </CheckBox>
      <Button fx:id="createButton" layoutX="85.0" layoutY="126.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="130.0" text="Create" />
      <Label fx:id="errorLabel" alignment="CENTER" layoutX="25.0" layoutY="159.0" prefHeight="35.0" prefWidth="250.0" textAlignment="CENTER" textFill="#e9e9e9" wrapText="true">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
